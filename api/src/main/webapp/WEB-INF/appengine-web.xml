<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

<!--   <application>stockholm-framework-template</application> -->
<!--   <version>default</version> -->

  <threadsafe>true</threadsafe>
  <sessions-enabled>false</sessions-enabled>
  <runtime>java8</runtime>

  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
    
    <!-- Required for Bean Validation  -->
    <!-- https://stackoverflow.com/a/53627367 -->
    <property name="javax.el.ExpressionFactory" value="com.sun.el.ExpressionFactoryImpl" />
  </system-properties>
  
  <static-files>
    <include path="index.html" expiration="1m"></include>
    <include path="*.json" expiration="30d"></include>
    <include path="assets/*.png" expiration="30d"></include>
    <include path="assets/locale/*.xlf" expiration="1m"></include>
    <include path="*.css" expiration="30d"></include>
    <include path="*.js" expiration="30d"></include>
    <include path="*.txt" expiration="30d"></include>
  </static-files>

</appengine-web-app>
